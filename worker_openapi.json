  "components": {
    "schemas": {
      "BenchmarkToolRequest": {
        "properties": {
          "script_path": {
            "type": "string",
            "title": "Script Path",
            "description": "Path to the script containing the build() function.",
            "default": "script.py"
          },
          "script_content": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Script Content",
            "description": "Direct content of the script."
          }
        },
        "type": "object",
        "title": "BenchmarkToolRequest",
        "description": "Request to run a benchmark tool (simulate, validate, etc.)."
      },
      "BenchmarkToolResponse": {
        "properties": {
          "success": {
            "type": "boolean",
            "title": "Success"
          },
          "message": {
            "type": "string",
            "title": "Message"
          },
          "artifacts": {
            "anyOf": [
              {
                "additionalProperties": true,
                "type": "object"
              },
              {
                "type": "null"
              }
            ],
            "title": "Artifacts"
          }
        },
        "type": "object",
        "required": [
          "success",
          "message"
        ],
        "title": "BenchmarkToolResponse",
        "description": "Response from a benchmark tool."
      },
      "Body_upload_file_fs_upload_file_post": {
        "properties": {
          "path": {
            "type": "string",
            "title": "Path"
          },
          "file": {
            "type": "string",
            "format": "binary",
            "title": "File"
          }
        },
        "type": "object",
        "required": [
          "path",
          "file"
        ],
        "title": "Body_upload_file_fs_upload_file_post"
      },
      "EditFileRequest": {
        "properties": {
          "path": {
            "type": "string",
            "minLength": 1,
            "title": "Path"
          },
          "edits": {
            "items": {
              "$ref": "#/components/schemas/EditOp"
            },
            "type": "array",
            "minItems": 1,
            "title": "Edits"
          }
        },
        "type": "object",
        "required": [
          "path",
          "edits"
        ],
        "title": "EditFileRequest",
        "description": "Request to edit a file with one or more operations."
      },
      "EditOp": {
        "properties": {
          "old_string": {
            "type": "string",
            "minLength": 1,
            "title": "Old String"
          },
          "new_string": {
            "type": "string",
            "title": "New String"
          }
        },
        "type": "object",
        "required": [
          "old_string",
          "new_string"
        ],
        "title": "EditOp",
        "description": "A single edit operation (find and replace)."
      },
      "ExecuteRequest": {
        "properties": {
          "code": {
            "type": "string",
            "minLength": 1,
            "title": "Code"
          },
          "timeout": {
            "type": "integer",
            "maximum": 300.0,
            "minimum": 1.0,
            "title": "Timeout",
            "default": 30
          }
        },
        "type": "object",
        "required": [
          "code"
        ],
        "title": "ExecuteRequest",
        "description": "Request to execute Python code."
      },
      "ExecuteResponse": {
        "properties": {
          "stdout": {
            "type": "string",
            "title": "Stdout"
          },
          "stderr": {
            "type": "string",
            "title": "Stderr"
          },
          "exit_code": {
            "type": "integer",
            "title": "Exit Code"
          },
          "timed_out": {
            "type": "boolean",
            "title": "Timed Out",
            "default": false
          }
        },
        "type": "object",
        "required": [
          "stdout",
          "stderr",
          "exit_code"
        ],
        "title": "ExecuteResponse",
        "description": "Response from executing Python code."
      },
      "FileInfo": {
        "properties": {
          "path": {
            "type": "string",
            "title": "Path"
          },
          "name": {
            "type": "string",
            "title": "Name"
          },
          "is_dir": {
            "type": "boolean",
            "title": "Is Dir"
          },
          "size": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "title": "Size"
          }
        },
        "type": "object",
        "required": [
          "path",
          "name",
          "is_dir"
        ],
        "title": "FileInfo",
        "description": "Information about a file or directory."
      },
      "GitCommitRequest": {
        "properties": {
          "message": {
            "type": "string",
            "minLength": 1,
            "title": "Message"
          }
        },
        "type": "object",
        "required": [
          "message"
        ],
        "title": "GitCommitRequest",
        "description": "Request to commit changes in the session workspace."
      },
      "GitCommitResponse": {
        "properties": {
          "success": {
            "type": "boolean",
            "title": "Success"
          },
          "commit_hash": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Commit Hash"
          },
          "message": {
            "type": "string",
            "title": "Message"
          }
        },
        "type": "object",
        "required": [
          "success",
          "message"
        ],
        "title": "GitCommitResponse",
        "description": "Response from a git commit operation."
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "LintRequest": {
        "properties": {
          "path": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Path",
            "description": "Path to file to lint (if in filesystem)."
          },
          "content": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Content",
            "description": "Direct content to lint."
          }
        },
        "type": "object",
        "title": "LintRequest",
        "description": "Request to lint Python code."
      },
      "LintResponse": {
        "properties": {
          "success": {
            "type": "boolean",
            "title": "Success"
          },
          "errors": {
            "items": {
              "additionalProperties": true,
              "type": "object"
            },
            "type": "array",
